<h1>Registro de Alumnos</h1><br>
	<%= form_for(@binnacle) do |f| %>

		  <div class="form-group">
		    <label for="text">Nombre:</label>
		    <%= f.collection_select(:nombre_student, Student.all,:nombre, :nombre, :class => 'form-control') %>
		  </div>

      <div class="form-group">
		    <label for="text">Nombre Equipo:</label>
        <%= f.collection_select(:nombre_computer, Computer.all,:equipo, :equipo, :class => 'form-control') %>

		  </div>
		  <%= f.submit "Registrarme" , class:"btn btn-default" %>
	<%end%>
